{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "eol-last": 0,
        "no-shadow": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "consistent-return" : 0,
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "import/no-extraneous-dependencies": 0
    }
}
